<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hsp.domain">
	<class name="demo.HibernateDemoVO" table="FRIEND">
		<id name="mem_id" column="mem_id" type="java.lang.Integer">
			<generator class="native" />
		</id>
		<property name="friend_id" column="friend_id" type="java.lang.Integer" />
		<property name="status" column="status" type="java.lang.Integer" />
	</class>
</hibernate-mapping>